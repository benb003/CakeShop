@model HomeViewModel



<partial name="_Carousel"></partial>
<div class="text-center">
    <br/>
    <br/>
    <h1>Cakes of the week</h1>
    <h5>Enjoy a weekly selection of our favorite cakes</h5>

    <div class="row pies-of-the-week">
        <div class="row row-cols-1 row-cols-md-3 g-4">
            @foreach (var cake in Model.CakesOfTheWeek)
            {
                <partial name="_CakeCard" model="cake"></partial>
            }
        </div>

    </div>
</div>
